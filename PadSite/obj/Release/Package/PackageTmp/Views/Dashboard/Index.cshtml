@{
    ViewBag.Title = ConfigSetting.SiteName + "运营管理系统";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

<div class="admin-content" id="dashboard">
    <div class="form-block">
        <div class="form-block-title">
            <h5>@ViewBag.Title</h5>
        </div>
        <div class="form-block-content">
            <div class="alert alert-block alert-info">
                <h4>您的角色:@Model.GroupName</h4>
            </div>


            <div class="alert alert-block alert-info">
                <h4>登录情况!</h4>
                <p>登录总数：@Model.LoginCount 次</p>
                <p>本次登录IP：@Model.CurrentIP</p>
                <p>本次登录时间：@Model.CurrentTime</p>
                <p>上次登录IP：@Model.LastIP</p>
                <p>上次登录时间：@Model.LastTime</p>
            </div>

        </div>
    </div>
</div>
