    @Html.Partial("_DisplayMessage")
    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        $(function () {
            var leftmenu = $('#leftmenu').kendoPanelBar({
                expandMode: "single",
                select: onSelect
            }).data("kendoPanelBar");

            function onSelect(e) {
                var $link = $(e.item);
                if (!$link.find('ul').length > 0) {
                    $('#loading').show();
                    $('#content').hide();
                    $('#dashboard').hide();
                }
            }
            $('#content').load(function () {
                $('#loading').hide();
                $(this).show();
            })
        })
    </script>
</body>
</html>
