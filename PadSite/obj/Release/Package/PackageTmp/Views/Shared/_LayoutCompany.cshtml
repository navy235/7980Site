@using StackExchange.Profiling;
<!DOCTYPE html>
<html>
<head>
    @this.InitClientTimings()
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
    <!--[if IE 7]>
      <link rel="stylesheet" href="netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome-ie7.min.css">
    <![endif]-->
    @Styles.Render("~/Content/kendo/2012.3.1114/css")
    @Styles.Render("~/Content/company")
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/kendo/2012.3.1114/jquery.min.js"></script>
    <script src="~/Scripts/kendo/2012.3.1114/kendo.all.min.js"></script>
    <script src="~/Scripts/kendo/2012.3.1114/kendo.aspnetmvc.min.js"></script>
    <script src="~/Scripts/kendo/2012.3.1114/cultures/kendo.culture.zh-CHS.min.js"></script>
    @Scripts.Render("~/bundles/effectcontrol")
    @Scripts.Render("~/bundles/jquerycontrol")
    @Scripts.Render("~/bundles/base")
    @Html.Partial("_CommonJavascript")
    @RenderSection("topscripts", required: false)
</head>
<body>
    @Html.Partial("_DisplayMessage")
    @Html.Partial("_TopNav")
    @RenderBody()
    @Html.Partial("_CompanyFooter")
    @RenderSection("scripts", required: false)
    @MiniProfiler.RenderIncludes(position: RenderPosition.Right, showTrivial: false, showTimeWithChildren: false)
</body>
</html>
